{
  "expo": {
    "name": "NEXURA-AI",
    "slug": "nexura-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nexura.ai"
    },
    "android": {
      "package": "com.nexura.ai",
      "permissions": [
        "READ_SMS",
        "RECEIVE_SMS",
        "NOTIFICATIONS",
        "READ_PHONE_STATE",
        "READ_CALL_LOG",
        "READ_CONTACTS",
        "CALL_PHONE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      "expo-secure-store",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "922c4310-994a-49b1-be29-59a946f84807"
      }
    }
  }
}
